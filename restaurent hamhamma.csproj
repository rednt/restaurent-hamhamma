<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<PlatformTarget>x86</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Pages\images\4.jpg" />
	  <None Remove="Pages\images\brik.jpg" />
	  <None Remove="Pages\images\chakchouka.jpg" />
	  <None Remove="Pages\images\Fricassé.jpg" />
